{
  "require": {
    "craftcms/cms": "3.5.12.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "2.8.1",
    "topshelfcraft/environment-label": "3.2.0",
    "ether/seo": "3.6.6",
    "wrav/oembed": "1.3.4",
    "xpertbot/craft-wheelform": "2.5.2",
    "pennebaker/craft-architect": "2.3.3",
    "verbb/super-table": "2.6.3",
    "charliedev/section-field": "1.1.0",
    "studioespresso/craft-splashingimages": "2.1.1",
    "rias/craft-schema": "1.0.3",
    "nystudio107/craft-minify": "1.2.10",
    "nystudio107/craft-typogrify": "1.1.18",
    "craftcms/feed-me": "4.2.4",
    "joomkit/pdfthumbnailer": "1.0.0",
    "verbb/icon-picker": "1.1.9",
    "ether/tags": "1.0.7",
    "verbb/smith": "1.1.8",
    "aelvan/imager": "v2.4.0",
    "dolphiq/iconpicker": "1.0.5",
    "superbig/craft3-mobiledetect": "1.0.2",
    "hybridinteractive/craft-position-fieldtype": "1.0.16",
    "xpertbot/wheelformhelper": "*",
    "nystudio107/craft-seomatic": "3.3.21"
  },
  "repositories": [
    {
      "type": "path",
      "url": "plugins/joomkit/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "plugins/xpertbot/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
