<!-- navigation -->
<header class="sticky-top">
    <!-- top header -->
    <div class="top-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="list-inline text-lg-right text-center">
                        <li class="list-inline-item">
                            <a href="mailto:info@companyname.com">Agencies Referral</a>
                        </li>
                        <li class="list-inline-item">
                            <a href="callto:0300 123 1550">Call Us:
                                <span class="ml-2"> 0300 123 1550</span>
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#" class="search" id="searchOpen">
                                <i id="searchicon" class="ti-search"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- nav bar -->
    <div class="navigation">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <a class="navbar-brand" href="{{ alias('@web') }}">
                    <img src="{{ baseUrl }}images/logo.png" alt="logo">
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    {% include "_partials/navbar.twig" %}
                </div>
            </nav>
        </div>
    </div>
</header>
