{% extends "_layouts/generic-page-layout.twig" %}

{% block headLinks %}
    {{ parent() }}
    <link rel="amphtml" href="{{ siteUrl('/amp') }}">
{% endblock headLinks %}

{% block content %}
    {% include "_partials/hero-slider.twig" %}

{% endblock %}

{% block subcontent %}


    <!-- sectionrow -->
    {% for block in entry.sectionRow.all() %}

        {% include ('home_blocks/' ~ block.type ~ '.twig') %}

    {% endfor %}

{% endblock %}

{# -- Any JavaScript that should be included before </body> -- #}
{% block bodyJs %}
    {{ parent() }}
{% endblock bodyJs %}
